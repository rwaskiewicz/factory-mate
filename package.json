{
  "name": "factory-mate",
  "version": "1.3.2",
  "description": "TypeScript library for building domain objects",
  "keywords": [
    "unit testing",
    "typescript",
    "factory",
    "builder",
    "tdd",
    "bdd",
    "test",
    "fixture"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint  \"src/**/*.ts\"",
    "test": "karma start",
    "testSingleRun": "karma start --single-run",
    "package": "rm -rf ./dist && tsc && npm pack",
    "report-coverage": "cat ./coverage/lcov/lcov.info | coveralls"

  },
  "author": "Ryan Waskiewicz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rwaskiewicz/factory-mate"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "base64-js": "^1.0.2",
    "coveralls": "^2.13.1",
    "ieee754": "^1.1.4",
    "isarray": "^1.0.0",
    "jasmine": "^2.6.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-typescript": "^3.0.1",
    "tslint": "^5.4.2",
    "typescript": "^2.4.0"
  }
}
